{
  "caption": "Network Proxy",
  "name": "network_proxy",
  "description": "The network proxy object describes a network proxy. Defined by D3FEND <a target='_blank' href='https://d3fend.mitre.org/dao/artifact/d3f:ProxyServer/'>d3f:ProxyServer</a>.",
  "extends": "object",
  "attributes": {
    "hostname": {
      "description": "The proxy hostname.",
      "requirement": "optional"
    },
    "ipv4": {
      "description": "The proxy IPv4 address.",
      "requirement": "recommended"
    },
    "ipv6": {
      "description": "The proxy IPv6 address.",
      "requirement": "recommended"
    },
    "port": {
      "description": "The proxy IP port.",
      "requirement": "required"
    },
    "svc_name" :{
      "description": "The name of the proxy service.",
      "requirement": "optional"
    },
    "uid" :{
      "description": "The unique identifier of the proxy.",
      "requirement": "recommended"
    }
  },
  "constraints": {
    "at_least_one": [
      "ipv4",
      "ipv6"
    ]
  }
}
